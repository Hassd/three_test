.address {
    height: 100%;
    width: 100%;
    .address_box {
        width: 50%;
        background-color: rgba(62, 149, 201, 0.6);
    }
    .address_search {
        .search_label {
            height: 30px;
            .lh(38, 12);
            color: #fff;
        }
        input[type="text"] {
            border: 1px solid #fff;
            border-radius: 2px;
            text-indent: 10px;
            color: #fff;
        }
        button {
            width: 100px;
            .lh(38, 14);
            color: #fff;
            border-radius: 19px;
            background-color: rgba(97, 203, 246, 1);

            &:hover {
                background-color: rgb(78, 200, 248);
            }
            &:active {
                background-color: rgb(40, 185, 243);
            }
        }
    }
    .operate_list {
        transition: all 0.3s;
        .lh(44, 12);
        color: #fff;
        overflow: hidden;
        i,
        span {
            display: block;
            cursor: pointer;
            transition: all 0.3s;
            &:hover {
                transform: scale(1.5);
            }
        }
    }
    .head_btn {
        .address_h3 {
            height: 42px;
            line-height: 42px;
        }
        button {
            .btn(
                118,
                42,
                rgba(97, 203, 246, 1),
                rgba(97, 203, 246, 1),
                #fff,
                22,
                14
            );
        }
        .btn1 {
            background-color: rgba(238, 238, 238, 1);
            border: 1px solid rgba(238, 238, 238, 1);
            color: rgba(153, 151, 151, 1);
        }
    }
    .between_radio {
        .patl(0, 0);
        width: 100%;
        height: 100%;
        background-color: rgba(0, 0, 0, 0.5);
        border-radius: 5px;
        display: flex;
        .iconfont {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            border: 1px solid rgba(255, 255, 255, 0.5);
            color: #fff;
            font-size: 40px;
            cursor: pointer;
            margin: auto;
            text-align: center;
            line-height: 80px;
            animation: Shake 0.1s infinite linear alternate;
        }

        @keyframes Shake {
            0% {
                transform: rotate(-5deg);
            }

            100% {
                transform: rotate(5deg);
            }
        }
    }
    .address_null {
        width: 100%;
        height: 110px;
        color: #fff;
        font-size: 12px;
        line-height: 110px;
        text-align: center;
    }
    .address_div {
        width: 346px;
        height: 110px;
        box-sizing: border-box;
        padding: 14px 14px 11px 14px;
        background: rgba(138, 187, 216, 0.8);
        box-shadow: 0px 3px 6px 0px rgba(68, 181, 250, 0.2);
        border-radius: 5px;
        overflow: hidden;
        position: relative;
        &::after {
            content: "";
            display: block;
            width: 100%;
            .patl(0, 0);
            height: 4px;
            background: linear-gradient(
                90deg,
                rgba(71, 194, 243, 1) 25%,
                rgba(112, 254, 74, 1) 25%,
                rgba(112, 254, 74, 1) 50%,
                rgba(253, 196, 74, 1) 50%,
                rgba(253, 196, 74, 1) 75%,
                rgba(254, 115, 97, 1) 75%,
                rgba(254, 115, 97, 1) 100%
            );
        }

        .user_info {
            .user_phone,
            .user_name {
                .lh(16, 13);
                color: #fff;
            }
        }
        p {
            color: #fff;
            font-size: 13px;
            line-height: 18px;
            height: 36px;
        }
        .default {
            .lh(12, 12);
            color: #fff;
            cursor: pointer;
        }
    }
    .select {
        // background: rgba(71, 194, 243, 1);
        &::after {
            background: rgba(71, 194, 243, 1);
        }
    }
    .title {
        width: 187px;
        .lh(18, 18);
        color: #fff;
        font-weight: 700;
    }
    .selectBackground {
        background-color: rgba(71, 194, 243, 1);
    }
}

