@import "./public.less";
.box1 {
    background: rgba(252, 254, 255, 1);
    border-radius: 60px 10px 10px 10px;
}
.box2 {
    background: rgba(252, 254, 255, 1);
    border-radius: 10px;
}
.box3 {
    background: rgba(252, 254, 255, 1);
    border-radius: 5px;
}
.box {
    padding: 15px 18px;
    box-sizing: border-box;
    .boxTitle {
        position: relative;
        h3 {
            color: #fff;
            font-weight: 700;
            font-size: 14px;
            height: 26px;
            line-height: 26px;
            letter-spacing: 2px;
        }
        .decorate {
            width: 5px;
            height: 26px;
            background-color: rgba(68, 181, 250, 1);
            position: absolute;
            top: 0;
            left: -18px;
        }
        .opBtn {
            height: 21px;
            line-height: 21px;
            background-color: #fff;
            border: 1px solid rgba(3, 146, 229, 1);
            color: rgba(3, 146, 229, 1);
            border-radius: 12px;
            font-size: 12px;
            width: auto;
            padding: 0 15px;
            margin-top: 2px;
            margin-left: 12px;
            cursor: pointer;
            letter-spacing: 2px;
            font-size: 12px;
        }
        .more {
            position: absolute;
            top: 0;
            right: 0;
            font-size: 14px;
            height: 26px;
            line-height: 26px;
            i {
                transform: rotate(-90deg);
                display: inline-block;
                font-size: 12px;
                margin-left: 5px;
            }
            cursor: pointer;
        }
    }
}
.shop_head {
    background: rgba(252, 254, 255, 1);
    box-shadow: 0px 6px 8px 1px rgba(213, 210, 210, 0.2);
    opacity: 0.9;
    border-radius: 50px;
    height: 100px;
    box-sizing: border-box;
    padding: 7px 14px;
    padding-right: 160px;
    position: relative;
    .flex_box {
        max-width: 400px;
    }
    .head_logo {
        .circular(86);
        position: relative;
        img {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            .circular(61);
        }
    }
    .head_name {
        .lh(86, 14);
        font-weight: 700;
        color: #333;
    }
    .head_btn {
        .patr(30, 40);
        .btn(125, 38, rgba(255, 93, 76, 1), rgba(255, 93, 76, 1), #fff, 20, 12);
        display: flex;
        i {
            display: block;
            color: #fff;
            .lh(38, 16);
        }
        span {
            display: block;
            color: #fff;
            .lh(38, 14);
        }
    }
}
textarea,
input::-webkit-input-placeholder {
    color: #ddd;
    font-size: 12px;
}
textarea,
input::-moz-placeholder {
    color: #ddd;
    font-size: 12px;
}
textarea,
input:-ms-input-placeholder {
    color: #ddd;
    font-size: 12px;
}

.infoGJ {
    width: 100%;
    height: 100%;
    padding-bottom: 20px;
    box-sizing: border-box;
    .info_null {
        width: 100%;
        height: 100px;
        line-height: 100px;
        text-align: center;
        color: #fff;
        font-size: 12px;
    }
    .wt-shoucang2{
        cursor: pointer;
    }
    .wt-shoucang1{
        cursor: pointer;
        color: rgba(253, 151, 69, 1);
    }
    .new_box {
        width: 100%;
        padding: 10px;
        cursor: pointer;
        &:hover {
            background-color: rgba(27, 125, 203, 1);
        }
        .newContent {
            width: 100%;
            height: 14px;
            padding: 0 20px;
            line-height: 14px;
            font-size: 14px;
            font-weight: bold;
            color: rgba(191, 236, 255, 1);
            cursor: pointer;
            box-sizing: border-box;
        }
        .new_text,
        .new_text2 {
            color: rgba(255, 255, 255, 1);
            display: inline-block;
        }
        .renew {
            width: 56px;
            height: 12px;
            font-size: 12px;
            font-weight: 400;
            color: rgba(191, 236, 255, 1);
            margin-left: 140px;
        }
        .newtime {
            box-sizing: border-box;
            padding: 0 20px;
            width: 100%;
            height: 15px;
            font-size: 11px;
            font-weight: 400;
            color: rgba(255, 255, 255, 1);
        }
    }
    .select {
        background-color: rgba(27, 125, 203, 1);
    }
    .infoGJ_box {
        width: 320px;
        border-radius: 5px;
        border: 1px solid rgb(255, 255, 255);
        height: 100%;
        position: relative;
        flex-shrink: 0;
        box-sizing: border-box;
        .scroll_box {
            height: 100%;
            overflow-x: hidden;
            padding: 2px 0 0 0;
        }
    }
    .infoGJ2_box {
        margin: -70px 0 0 0;
        padding: 0px 40px;
        height: 100%;
        .test {
            width: 100%;
            height: 40px;
            font-size: 28px;
            font-weight: bold;
            text-align: center;
            // border: 1px solid red;
            color: rgba(255, 255, 255, 1);
        }
        .time {
            width: 100%;
            height: 14px;
            line-height: 14px;
            font-size: 14px;
            color: #ddd;
            font-weight: 400;
            text-align: center;
        }
        .news_right {
            width: 100%;
            height: 100%;
            font-size: 16px;
            word-wrap: break-word;
            line-height: 1.8em;
            // border: 1px solid red;
            p {
                font-weight: 400;
                color: rgba(208, 255, 253, 1);
            }
            img {
                max-width: 300px;
                max-height: 300px;
            }
        }
    }
}

.resetBtn {
    width: 60px;
    height: 60px;
    line-height: 60px;
    border-radius: 50%;
    background-color: rgba(4, 85, 137, 0.5);
    box-shadow: inset 0 0 20px #ddd;
    position: fixed;
    // bottom: 10px;
    // right: 10px;
    bottom: 145px;
    left: 7px;
    z-index: 10;
    color: #fff;
    font-size: 14px;
    user-select: none;
    cursor: pointer;
}
.mouse_introduce {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 250px;
    user-select: none;
    height: 140px;
    img {
        width: 250px;
        height: 140px;
    }
}

.list_paging {
    align-items: center;
    button,
    .pagingNumBox div {
        padding: 0 10px;
        height: 30px;
        line-height: 30px;
        font-size: 14px;
        background-color: rgba(62, 149, 201, 0.8);
        margin-left: 5px;
        color: #fff;
        border-radius: 2px;
        user-select: none;
        &:hover {
            background-color: rgba(62, 149, 201, 0.6);
            color: #fff;
        }
    }
    .pagingNumBox div {
        cursor: pointer;
    }
    .pagingTotal {
        height: 30px;
        line-height: 30px;
        font-size: 12px;
        color: #fff;
        margin-left: 15px;
    }
    .pagingNumBox .paging_select {
        background-color: #fff;
        color: rgba(62, 149, 201, 1);
    }
}
.listBox {
    height: 600px;
    background-color: rgba(62, 149, 201, 0.8);
    .list_title {
        width: 95%;
        margin: 0 auto;
        color: #fff;
    }
    .list_data {
        width: 95%;
        flex: 1;
        margin: 0 auto;
        background-color: rgba(62, 149, 201, 0.8);
    }
    .list_tr {
        width: 95%;
        margin: 0 auto;
        display: flex;
        justify-content: flex-start;
    }
    .input {
        border: 1px solid #ddd;
        text-align: center;
        height: 25px;
        line-height: 25px;
        color: #fff;
        font-size: 12px;
    }
    .button {
        margin-left: 5px;
        width: 70px;
        height: 27px;
        line-height: 25px;
        color: #fff;
        text-align: center;
        border-radius: 4px;
        background-color: rgba(255, 255, 255, 0.6);
        border: 1px solid #fff;
        color: #333;
        font-size: 12px;
        &:hover {
            background-color: rgba(255, 255, 255, 1);
        }
    }
    .input_div {
        height: 25px;
        line-height: 25px;
    }
    .list_th {
        height: 40px;
        line-height: 40px;
        background-color: rgba(62, 149, 201, 0.8);
        border-bottom: 1px solid #fff;
        font-size: 14px;
    }
    .list_null {
        width: 100%;
        text-align: center;
        height: 150px;
        margin: 0 auto;
        line-height: 150px;
        color: #fff;
        background-color: rgba(62, 149, 201, 0.8);
        font-size: 12px;
        font-style: oblique;
        user-select: none;
    }
    .list_td,
    .list_th {
        div {
            user-select: none;
            width: 150px;
            color: #fff;
            flex: 1;
            text-align: center;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }
    }
    .list_td {
        width: 100%;
        height: 30px;
        line-height: 30px;
        font-size: 12px;
        &:nth-child(2) {
            background-color: rgba(255, 255, 255, 0.4);
        }
    }
}

